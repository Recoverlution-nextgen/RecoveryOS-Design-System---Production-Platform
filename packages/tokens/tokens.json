    "48": "48px",
    "64": "64px",
    "80": "80px",
    "96": "96px",
    "128": "128px"
  },
  "primitive-radius": {
    "none": "0px",
    "sm": "2px",
    "md": "4px",
    "lg": "6px",
    "xl": "8px",
    "full": "9999px"
  },
  "primitive-font-size": {
    "xs": "12px",
    "sm": "14px",
    "base": "16px",
    "lg": "18px",
    "xl": "20px",
    "2xl": "24px",
    "3xl": "30px",
    "4xl": "36px"
  },
  "primitive-font-weight": {
    "normal": "400",
    "medium": "500",
    "semibold": "600",
    "bold": "700"
  },
  "semantic-colors": {
    "surface-base": "{primitive-colors.gray-900}",
    "surface-elevated": "{primitive-colors.gray-800}",
    "ink-primary": "{primitive-colors.white}",
    "ink-secondary": "{primitive-colors.gray-300}",
    "ink-tertiary": "{primitive-colors.gray-400}",
    "ink-inverse": "{primitive-colors.gray-900}",
    "accent-primary": "{primitive-colors.blue-400}",
    "accent-warm": "{primitive-colors.orange-400}",
    "border-subtle": "{primitive-colors.gray-700}",
    "border-default": "{primitive-colors.gray-600}",
    "state-safe": "{primitive-colors.green-400}",
    "state-caution": "{primitive-colors.yellow-400}",
    "state-alert": "{primitive-colors.red-400}"
  },
  "semantic-spacing": {
    "xs": "{primitive-spacing.2}",
    "sm": "{primitive-spacing.4}",
    "md": "{primitive-spacing.8}",
    "lg": "{primitive-spacing.12}",
    "xl": "{primitive-spacing.16}",
    "2xl": "{primitive-spacing.24}",
    "3xl": "{primitive-spacing.32}",
    "4xl": "{primitive-spacing.48}",
    "5xl": "{primitive-spacing.64}",
    "6xl": "{primitive-spacing.96}"
  },
  "component-colors": {
    "return-surface": "{semantic-colors.surface-elevated}",
    "return-ink": "{semantic-colors.ink-primary}",
    "return-focus-ring": "{semantic-colors.accent-primary}",
    "return-press": "{semantic-colors.accent-warm}",
    "return-glow": "rgba(79, 179, 255, 0.2)",
    "thread-line": "{semantic-colors.border-subtle}",
    "thread-node": "{semantic-colors.accent-primary}",
    "thread-pulse": "{semantic-colors.accent-warm}",
    "trace-surface": "{semantic-colors.surface-elevated}",
    "trace-border": "{semantic-colors.border-subtle}",
    "trace-stamp": "{semantic-colors.accent-primary}"
  },
  "component-spacing": {
    "return-padding": "{semantic-spacing.md}",
    "return-margin": "{semantic-spacing.sm}",
    "thread-node-spacing": "{semantic-spacing.lg}",
    "trace-padding": "{semantic-spacing.md}"
  },
  "motion": {
    "arrive": {
      "duration": 240,
      "easing": "cubic-bezier(0.19, 0.8, 0.2, 1)"
    },
    "glide": {
      "duration": 360,
      "easing": "cubic-bezier(0.4, 0, 0.2, 1)"
    },
    "settle": {
      "duration": 480,
      "easing": "cubic-bezier(0.19, 1, 0.22, 1)"
    },
    "confirm": {
      "duration": 180,
      "easing": "cubic-bezier(0.19, 0.8, 0.2, 1)"
    },
    "carry": {
      "duration": 300,
      "easing": "cubic-bezier(0.4, 0, 0.2, 1)"
    }
  }
}
